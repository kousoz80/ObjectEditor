// loadコマンド
int cmd_load() throws BasicError{
  String buf;
  Program prog = new Program();

  if( TokenType != STRING ) assertError( SyntaxError );
  String fname = TokenText.toString();
  cmd_new();
  try{
    BufferedReader din = new BufferedReader( new FileReader( fname ) );
    while( ( buf = din.readLine() ) != null ){
      devide_line( prog, new String( buf+&quot;\r&quot; ) );
      if( prog.lineno != 0 &amp;&amp; !prog.text.equals(&quot;&quot;) ) edit_line( prog.lineno, prog.text );
    }
    din.close();
  } catch( IOException ie ){ assertError(&quot;can not load \&quot;&quot; + fname + &quot;\&quot;&quot; ); }
  return TERMINATE;
}


// saveコマンド
int cmd_save() throws BasicError{
  Program p;
  if( TokenType != STRING ) assertError( SyntaxError );
  String fname = TokenText.toString();
  try{
    BufferedWriter dout = new BufferedWriter( new FileWriter( fname ) );
    for( p = TopProg; p != null; p = (Program)p.next ){
      String txt = p.text;
      dout.write( p.lineno + &quot; &quot; + txt.substring(0,txt.length()-1) + &quot;\n&quot;);
    }
    dout.close();
  } catch( IOException ie ){ assertError(&quot;can not save \&quot;&quot; + fname + &quot;\&quot;&quot; ); }
  return TERMINATE;
}


