// ifコマンド
int cmd_if() throws BasicError{

  // 論理式が真ならば&quot;thenをチェックしてその次から始める&quot;
  eval_lexpression();
  if( (int)get_number() != 0 ){
    checkToken(&quot;then&quot;);
    if( TokenType == NUMBER ){
      Program p = serch_line( (int)TokenValue );
      if( p == null ) assertError(&quot;irreagal line No&quot;);
      CurrentProg = p;
      TokenP = 0;
      getToken();
    }
    return DONE;
  }

  // 行のトークンを逐次検索する
  do{
    getToken();

    // &quot;else&quot;があったらそこから始める
    if( TokenText.toString().equals( &quot;else&quot; ) ){
      getToken();
      if( TokenType == NUMBER ){
        Program p = serch_line( (int)TokenValue );
        if( p == null ) assertError(&quot;irreagal line No&quot;);
        CurrentProg = p;
        TokenP = 0;
        getToken();
      }
      return DONE;
    }
  } while( TokenType != EOL );

  // &quot;else&quot;がなかったら完了
  return DONE;
}


// dimコマンド
int cmd_dim() throws BasicError{
  Variable var;
  String var_name;
  int[] dim = new int[MAX_DIMENSION];
  int dx, nn, i;
  while(true){
    if( TokenType != VARIABLE ) assertError( SyntaxError );
    var_name = TokenText.toString();
    getToken();
    checkToken(&quot;(&quot;);
    dx = 0;
    while(true){
      clear_value();
      eval_expression();
      if( dx &gt;= MAX_DIMENSION ) assertError(&quot;array dimension is too large&quot;);
      if( ( dim[ dx++ ] = (int)get_number() ) &lt;= 0 ) assertError( OutOfRange );
      if( TokenType == DELIMIT &amp;&amp; TokenText.charAt(0)  == &apos;)&apos; ) break;
      checkToken(&quot;,&quot;);
    }
    var = _variable( var_name );
    var.dimension = dx;
    for( nn = 1, i = 0; i &lt; dx; i++ ) nn *= ( var.dim[ i ] = dim[ i ] + 1 );

    // 文字列型配列を初期化
    if( var.value.type == STRING ){
      String[] p = new String[nn];
      for( i = 0; i &lt; nn; i++ ){
        p[ i ] = new String(&quot;&quot;);
      }
      var.value.array = p;
    }

    // 数値型配列を初期化
    else{
      var.value.array = new double[nn];
    }

    getToken();
    if( TokenType == DELIMIT &amp;&amp; TokenText.charAt(0)  !=&apos;,&apos; ) break;
    getToken();
  }
  getToken();
  return DONE;
}


// openコマンド
int cmd_open() throws BasicError{
  int io_flg = 0, i;
  String fname;
  clear_value();
  eval_expression();
  checkToken(&quot;for&quot;);
  if( TokenText.toString().equals( &quot;input&quot; ) ) io_flg = 0;
  else if( TokenText.toString().equals( &quot;output&quot; ) ) io_flg = 1;
  else assertError( SyntaxError );
  getToken();
  checkToken(&quot;as&quot;);
  checkToken(&quot;#&quot;);
  if( TokenType != NUMBER ) assertError( SyntaxError );
  if( ( i = (int)TokenValue ) &lt; 0 || i &gt;= MAX_FILES ) assertError( OutOfRange );
  getToken();
  fname = get_string();

  if( Xfd[ i ] != null )  assertError(&quot;file is already open&quot;);

  // 書きこみモード
  try{
    if( io_flg != 0 ){
      if( ( Xfd[ i ] = new BufferedWriter( new FileWriter( fname ) ) ) == null ){
        assertError(&quot;can\&apos;t open file(write)&quot;);
      }
    }

    // 読み込みモード
    else{
      if( ( Xfd[ i ] = new BufferedReader( new FileReader( fname ) ) ) == null ){
        assertError(&quot;can\&apos;t open file(read)&quot;);
      }
    }
  } catch( IOException e){ assertError(&quot;i/o error&quot;); }
  return DONE;
}


// closeコマンド
int cmd_close() throws BasicError{
  int i;
  if( TokenType == DELIMIT &amp;&amp; TokenText.charAt(0)  == &apos;#&apos; ){
    getToken();
    if( TokenType != NUMBER ) assertError( SyntaxError );
    if( ( i = (int)TokenValue ) &gt;= 0 &amp;&amp; i &lt; MAX_FILES ){
      try{
        if( Xfd[ i ] != null ){
          if( Xfd[ i ] instanceof BufferedReader ) ((BufferedReader)Xfd[ i ]).close();
          else if( Xfd[ i ] instanceof BufferedWriter ) ((BufferedWriter)Xfd[ i ]).close();
          Xfd[ i ] = null;
        }
      } catch( IOException e){ assertError(&quot;i/o error&quot;); }
    }
    getToken();
  }
  else{
    for( i = 0; i &lt; MAX_FILES; i++ ){
      try{
        if( Xfd[ i ] != null ){
          if( Xfd[ i ] instanceof BufferedReader ) ((BufferedReader)Xfd[ i ]).close();
          else if( Xfd[ i ] instanceof BufferedWriter ) ((BufferedWriter)Xfd[ i ]).close();
          Xfd[ i ] = null;
        }
      }
      catch( IOException e){ assertError(&quot;i/o error&quot;); }
    }
  }
  return DONE;
}


