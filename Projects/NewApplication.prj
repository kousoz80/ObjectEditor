    getToken();
  }


//Log.d(&quot;cmd_list&quot;, &quot;TokenText=&quot;+TokenText+&quot;, Token Type=&quot;+TokenType);

  if( TokenType == DELIMIT &amp;&amp; ( TokenText.charAt(0) == &apos;,&apos; || TokenText.charAt(0) == &apos;-&apos; ) ) getToken();
