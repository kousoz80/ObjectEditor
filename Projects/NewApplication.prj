/* 論理項の処理 */
int eval_lterm() throws BasicError{

  // 論理因子を解析
  eval_relation();
  for(;;){

    // 論理項は論理因子AND論理因子AND...
    if( TokenText.toString().equals( &quot;and&quot; ) ){
      getToken();
      eval_relation();
