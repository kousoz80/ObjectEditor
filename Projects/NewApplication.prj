/* 論理因子の処理 */
int eval_relation() throws BasicError{

  // 式を解析
  eval_expression();
  for(;;){

    // 論理因子は 式&lt;式
    if( TokenText.toString().equals( &quot;&lt;&quot; ) ){
      getToken();
      eval_expression();
      eval_cmp();
      eval_lt();
    }

    // 論理因子は 式&lt;&gt;式
    else if( TokenText.toString().equals( &quot;&lt;&gt;&quot; ) ){
      getToken();
      eval_expression();
      eval_cmp();
      eval_neq();
    }

    // 論理因子は 式&lt;=式
    else if( TokenText.toString().equals( &quot;&lt;=&quot; ) ){
      getToken();
      eval_expression();
      eval_cmp();
      eval_le();
    }

    // 論理因子は 式&gt;式
    else if( TokenText.toString().equals( &quot;&gt;&quot; ) ){
      getToken();
      eval_expression();
      eval_cmp();
      eval_gt();
    }

    // 論理因子は 式&gt;=式
    else if( TokenText.toString().equals( &quot;&gt;=&quot; ) ){
      getToken();
      eval_expression();
      eval_cmp();
      eval_ge();
    }

    // 論理因子は 式=式
    else if( TokenText.toString().equals( &quot;=&quot; ) ){
      getToken();
      eval_expression();
      eval_cmp();
      eval_eq();
    }

    // 上記以外ならば終了
    else return 0;
  }
//  return 0;
}


