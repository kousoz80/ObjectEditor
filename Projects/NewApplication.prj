<codetext>// 描画関数

    // 実際に画面に表示する
    public void _disp(){
      display.repaint();
    }
  
  
    public void disp_cls(){
      graphics.setColor( new Color( bak_r, bak_g, bak_b ) );
      graphics.fillRect( 0, 0, width, height );
      posx=0;
      posy=0;
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.fillRect(posx, posy+stepy-fontsize, 3, fontsize);    
    }


    public void disp_puts( String s ){
      for( int i = 0; i &lt; s.length(); i++ ){
        disp_putc( s.charAt( i ) );
      }
    }

    public void disp_putc( char c ){
      graphics.setColor( new Color(bak_r, bak_g, bak_b));
      graphics.fillRect(posx, posy+stepy-fontsize, 3, fontsize);    
      if( c == &apos;\n&apos; ){
        posx = 0;
        posy += stepy;
        if( posy &gt;= height-stepy-1 ){
//        disp_cls();
          graphics.drawImage( image, 0, 0, width, height-stepy, 0, stepy, width, height, display);
          posy -= stepy;
          graphics.setColor( new Color(bak_r, bak_g, bak_b));
          graphics.fillRect(0, posy, width+2, stepy+2);
        }
      }
      else{
        int textwidth;
        if( ( c&lt;=&apos;\u007e&apos; )||
            ( c==&apos;\u00a5&apos; )||
            ( c==&apos;\u203e&apos; )||
            ( c&gt;=&apos;\uff61&apos; &amp;&amp; c&lt;=&apos;\uff9f&apos; )
          )  textwidth = stepx/2+2;
        else textwidth = stepx;
        if( c&gt;=&apos; &apos; ){
          graphics.setColor( new Color(bak_r, bak_g, bak_b));
          graphics.fillRect(posx, posy, textwidth, stepy);
          graphics.setColor( new Color(for_r, for_g, for_b));
          graphics.drawString( c+&quot;&quot;, posx, posy+stepy);
          posx += textwidth;
          if( posx &gt;= width ){
            posx = 0;
            posy += stepy;
            if( posy &gt;= height-stepy-1 ){
//            disp_cls();
              graphics.drawImage( image, 0, 0, width-1, height-stepy, 0, stepy, width, height, display);
              posy -= stepy;
              graphics.setColor( new Color(bak_r, bak_g, bak_b));
              graphics.fillRect(0, posy, width+2, stepy+2);
            }
          }
        }
      }
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.fillRect(posx, posy+stepy-fontsize, 3, fontsize);    
    }


    public void disp_color1( int c ){
      for_r = ( c &gt;&gt; 16 ) &amp; 0xff;
      for_g = ( c &gt;&gt; 8  ) &amp; 0xff;
      for_b = c &amp; 0xff;
    }


    public void disp_color2( int c1, int c2 ){
      for_r = ( c1 &gt;&gt; 16 ) &amp; 0xff;
      for_g = ( c1 &gt;&gt; 8  ) &amp; 0xff;
      for_b = c1 &amp; 0xff;
      bak_r = ( c2 &gt;&gt; 16 ) &amp; 0xff;
      bak_g = ( c2 &gt;&gt; 8  ) &amp; 0xff;
      bak_b = c2 &amp; 0xff;
    }


    public void disp_screen1( int wid, int hei ){
      width  = wid;
      height = hei;
      image = new BufferedImage( width, height,  BufferedImage.TYPE_INT_RGB);
      graphics = image.createGraphics();
      graphics.setFont( new Font( &quot;Monospaced&quot;, Font.BOLD, fontsize ) ); 
      graphics.setColor( new Color(bak_r, bak_g, bak_b));
      graphics.fillRect(9, 0, width, height);
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.fillRect(posx, posy+stepy-fontsize, 3, fontsize);    
      disp_cls();_disp();
    }


    public void disp_screen2( int wid, int hei, int fnt ){
      width  = wid;
      height = hei;
      fontsize = fnt;
      image = new BufferedImage( width, height,  BufferedImage.TYPE_INT_RGB);
      graphics = image.createGraphics();
      graphics.setFont( new Font( &quot;Monospaced&quot;, Font.BOLD, fontsize ) ); 
      graphics.setColor( new Color(bak_r, bak_g, bak_b));
      graphics.fillRect(9, 0, width, height);
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.fillRect(posx, posy+stepy-fontsize, 3, fontsize);    
      disp_cls();_disp();
    }

    public void disp_locate( int x, int y ){
      graphics.setColor( new Color(bak_r, bak_g, bak_b));
      graphics.fillRect(posx, posy+stepy-fontsize, 3, fontsize);    
      posx=x * stepx;
      posy=y * stepy;
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.fillRect(posx, posy+stepy-fontsize, 3, fontsize);    
    }

    public void disp_pset( int x, int y ){
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.drawLine( x, y, x, y);
    }

    public void disp_line( int x0, int y0, int x1, int y1 ){
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.drawLine( x0, y0, x1, y1);
    }

    public void disp_box( int x0, int y0, int x1, int y1 ){
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.drawRect( x0, y0, x1-x0+1, y1-y0+1);
    }


    public void disp_boxf( int x0, int y0, int x1, int y1 ){
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.fillRect( x0, y0, x1-x0+1, y1-y0+1);
    }


    public void disp_circle( int x0, int y0, int x1, int y1 ){
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.drawOval( x0, y0, x1-x0+1, y1-y0+1);
    }


    public void disp_circlef( int x0, int y0, int x1, int y1 ){
      graphics.setColor( new Color(for_r, for_g, for_b));
      graphics.fillOval( x0, y0, x1-x0+1, y1-y0+1);
    }

    public void disp_image( int x, int y, String file ){
//      boolean sucsess = true;
//      BufferedImage readImage = null;
//
//      try {
//        readImage = ImageIO.read(new File(file));
//      } catch (Exception e) { sucsess = false;}
//      if( sucsess ) graphics.drawImage( readImage, x, y, display );
    }


    public void disp_start( String cmd ){
      Runtime runtime = Runtime.getRuntime();
      try {            //実行するコマンドを指定
        runtime.exec(cmd);
      } catch (IOException e) {} 
    }


    public void disp_exec( String cmd ){
      Runtime runtime = Runtime.getRuntime();
      Process process;
      String  line = &quot;&quot;;
      BufferedReader reader;
      try {            //実行するコマンドを指定
        process = runtime.exec(cmd);
        reader = new BufferedReader( new InputStreamReader(process.getInputStream()));
        while ((line = reader.readLine()) != null) {
          disp_puts( line + &quot;\n&quot; );_disp();
        }
        reader.close();
        process.waitFor();
      } catch (IOException e) {} catch (InterruptedException e) {}
    }


    public String disp_time(){
      String s = (new SimpleDateFormat(&quot;HH:mm:ss&quot; , Locale.US)).format(new Date());
      return s;
    }


    public String disp_date(){
      String s = (new SimpleDateFormat(&quot;yy/MM/dd&quot;, Locale.US)).format(new Date());
      return s;
    }


    /* 1行入力 */
    public String disp_gets(){
      String ret;
      while( InputBuffer.equals(&quot;&quot;) ){
        try{
          Thread.sleep(100, 0);
        }catch( InterruptedException e ){}
      }
      ret = new String( InputBuffer );
      disp_puts( InputBuffer+&quot;\n&quot; );
      InputBuffer = &quot;&quot;;
      return ret;
    }


    // キー入力を調べる
    public char disp_inkey(){
      char[] chr = Character.toChars( KeyCode );
      return chr[0];
    }


    // １文字入力
    public char disp_getch(){
      GetchChar = &apos;\0&apos;;
      is_getch = true;
      while( GetchChar == &apos;\0&apos; ){
        try{
          Thread.sleep(100, 0);
        }catch( InterruptedException e ){}
      }
      is_getch = false;
      return GetchChar;
    }


    // メディアファイルを再生する
    public void disp_play( String fname ){
      AudioFormat format = null;
      DataLine.Info info = null;
      Clip line = null;
      File audioFile = null;

      try{
        audioFile = new File(fname);
        format = AudioSystem.getAudioFileFormat(audioFile).getFormat();
        info = new DataLine.Info(Clip.class, format);
        line = (Clip)AudioSystem.getLine(info);
        line.open(AudioSystem.getAudioInputStream(audioFile));
        line.start();
    }
    catch(Exception e){}
  }
