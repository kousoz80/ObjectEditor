int cmd_run() throws BasicError{

//disp_puts( &quot;cmd_run: TokenText=&quot;+TokenText+&quot;, Token Type=&quot;+TokenType+&quot; , Code=&quot;+TokenCode+&quot;\n&quot;);
//_disp();
  int i;

    for( i = 0; i &lt; MAX_FILES; i++ ){
      try{
