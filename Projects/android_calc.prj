  else if( op0.equals(&quot;-&quot;) )reg1 = reg0 - reg1;
  else if( op0.equals(&quot;*&quot;) )reg1 = reg0 * reg1;
  else if( op0.equals(&quot;/&quot;) )reg1 = reg0 / reg1;
  op0   = op1;
  reg0 = reg1;
}
