else if( op1.equals(&quot;C&quot;) )    reg1 = 0.0;
else if( op1.equals(&quot;M+&quot;) )   mem += reg1;
else if( op1.equals(&quot;Mr&quot;) ){  reg1 = mem; mem = 0.0; }
else{
