<codetext>// 画像操作関数一覧


// 作業変数
 count jj#,ix#,iy#
 long pp#,qq#,px#,py#,gx#,gy#,gx1#,gy1#
 long tx#,ty#,rx#,ry#,ox#,oy#,vx#,vy#
 long vx1#,vy1#,qx#,qy#,co#

// 点を打つ
gdraw_point:
  py#= pop px#=
  if gcolor#=COLOR_CLEAR then end
  py#, gwidth#, * px#, +
  4, * bitmap#, + pp#=
  gcolor#, (pp)!=
  end


// 与えられた座標の色を返す
gget_point:
  py#= pop px#=
  py#, gwidth#, * px#, +
  4, * bitmap#, + pp#=
  (pp)!,  end

  
// 線を引く
// 使用法: gx, gy, gx1, gy1, gdraw_line
gdraw_line:
  gy1#= pop gx1#= pop gy#= pop gx#=
  if gcolor#=COLOR_CLEAR then end

gline:
  gx#, gx1#, - tx#= abs rx#=
  gy#, gy1#, - ty#= abs ry#=
  if ry#&gt;rx# goto gline_y
  if gx#=gx1# then gx#, gy#, gdraw_point end
  
  // モードX
  gline_x:
    1, rx#=
    if tx#&lt;0  then -1, rx#=
    for ix#=0 to tx# step rx#
      ix#, ty#,  * tx#, / gy1#, + ry#=
      ix#, gx1#, + ry#, gdraw_point
    next ix#
    end
    
  // モード　ｙ
  gline_y:
    1, ry#=
    if ty#&lt;0  then -1, ry#=
    for iy#=0 to ty# step ry#
      iy#, tx#,  * ty#, / gx1#, + rx#=
      iy#, gy1#, + rx#, swap gdraw_point
    next iy#
    end
    
// 画像を消去
ggcls:
  gwidth#,  gheight#, * 1, + tt#=
  for ii#=2 to tt#
    gcolor#, (bitmap)!(ii#)=
  next ii#
  end

// 長方形を描いてうめる
// 使用法: gx, gy, gx1, gy1, gfill_rect
gfill_rect: 
  gy1#= pop gx1#= pop gy#= pop gx#=
  if gcolor#=COLOR_CLEAR then end

  1, rx#= ry#=
  if gy#&lt;gy1# then -1, ry#=
  if gx#&lt;gx1# then -1, rx#=
  for iy#=gy1# to gy# step ry#
    for ix#=gx1# to gx# step rx#
      ix#,  iy#, gdraw_point
    next ix#
  next iy#
  end

// 長方形を描く
// 使用法: gx, gy, gx1, gy1, gdraw_rect
gdraw_rect: 
  gy1#= pop gx1#= pop gy#= pop gx#=
  if gcolor#=COLOR_CLEAR then end
  1, rx#= ry#=
  if gy#&lt;gy1# then -1, ry#=
  if gx#&lt;gx1# then -1, rx#=
  for ix#=gx1# to gx# step rx#
    ix#, gy#,   gdraw_point
    ix#, gy1#, gdraw_point
  next ix#
  for iy#=gy1# to gy# step ry#
    gx#,  iy#,  gdraw_point
    gx1#, iy#, gdraw_point
  next iy#
  end
  
// 楕円を描いてうめる
// 使用法: gx, gy, gx1, gy1, gfill_circle
gfill_circle:
  gdraw_circle
  ox1#, oy1#, gpaint
  end

// 楕円を描く
// 使用法: gx, gy, gx1, gy1, gdraw_circle
gdraw_circle:
  long ox1#,oy1#
  gy1#= pop gx1#= pop gy#= pop gx#=
  if gcolor#=COLOR_CLEAR then end
  gx#,  vx#=  gy#,  vy#=
  gx1#, vx1#= gy1#, vy1#=
  if gx1#&gt;gx# then gx1#, gx#, swap gx#= swap gx1#=
  if gy1#&gt;gy# then gy1#, gy#, swap gy#= swap gy1#=
  gx#, gx1#, + 2, / ox1#=
  gy#, gy1#, + 2, / oy1#=
  gx#, gx1#, - 2, / qx#=
  if qx#=0 then gline end
  gy#, gy1#, - 2, / qy#=
  if qy#=0 then gline end
  gx#, gx1#=  oy1#, gy1#=
  for ii#=0 to TABLE_N
    qx#, cos2table#(ii#), * 32767, / ox1#, + gx#=
    qy#, sin2table#(ii#), * 32767, / oy1#, + gy#=
    gline
    gx#, gx1#= gy#, gy1#=
  next ii#
  end


// 塗る
gpaint:
  const Q_SIZE   4096
  long   q_buf#(Q_SIZE)
  long   put_p#,get_p#
  
  gy#= pop gx#=
  if gcolor#=COLOR_CLEAR then end
  0, put_p#= get_p#=
  gx#, gy#, gget_point co#=
  if co#=gcolor# then end
  gx#, gy#, gput_pset

  gpaint1:  // うった点の座標を求める
    if get_p#=put_p# then end
    q_buf#(get_p#), vx#=   get_p#++
    q_buf#(get_p#), vy#=   get_p#++
    if get_p#&gt;=Q_SIZE then 0, get_p#=

    // うった点の四方にまた点をうつ
    vx#, 1, + vy#, gput_pset
    vx#, 1,  - vy#, gput_pset
    vy#, 1, + vx#, swap gput_pset
    vy#, 1,  -  vx#, swap gput_pset
  goto gpaint1
  
  //  点をうってその座標を記録する
  gput_pset:
    qy#= pop qx#=
    if qx#&lt;0     then end  // 範囲外ならしない
    if qx#&gt;=gwidth#  then end
    if qy#&lt;0     then end
    if qy#&gt;=gheight# then end
    qx#, qy#, gget_point co#=
    if co#=gcolor# then end // すでに点がうってあるときもしない
    qx#, qy#, gdraw_point
    qx#, q_buf#(put_p#)=  put_p#++
    qy#, q_buf#(put_p#)=  put_p#++
    if put_p#&gt;=Q_SIZE then 0, put_p#=
    end


// 画像を描画
// 使用法: gx, gy, address, gdraw_image
gdraw_image:
  qq#= pop gy#= pop gx#=
  if qq#=NULL then end
  (qq)!, rx#=
  if rx#&lt;0 then end
  if rx#&gt;=gwidth# then end
  qq#, 4, + qq#=
  (qq)!, ry#=
  if ry#&lt;0 then end
  if ry#&gt;=gheight# then end
  qq#, 4, + qq#=
  gx#, rx#, + 1, - gx1#=
  gy#, ry#, + 1, - gy1#=
  for ii#=gy# to gy1#
    for jj#=gx# to gx1#
      ii#, gwidth#, * jj#, +
      4, * bitmap#, + pp#=
      if (qq)$(3)=0 then (qq)!, (pp)!=
      qq#, 4, + qq#=
    next jj#
  next ii#
  jj#, ii#, end
    

// コピーエリアに画像をコピー
// 使用法: gx, gy, gx1, gy1, xcopy_image
gcopy_image:
  gy1#= pop gx1#= pop gy#= pop gx#=
  if gx1#&gt;=gwidth#  then gwidth#,  1, - gx1#= 
  if gy1#&gt;=gheight# then gheight#, 1, - gy1#= 
  copy_area#, qq#=
  gx1#, gx#, - 1, + (qq)!=
  qq#, 4, + qq#=
  gy1#, gy#, - 1, + (qq)!=
  qq#, 4, + qq#=
  for ii#=gy# to gy1#
    for jj#=gx# to gx1#
      ii#, gwidth#, * jj#, +
      4, * bitmap#, + pp#=
      (pp)!, (qq)!=
      qq#, 4, + qq#=
    next jj#
  next ii#
  end


// コピーエリアから貼り付ける
// 使用法: gx, gy, xpaste_image
gpaste_image:
  copy_area#, gdraw_image
  end
    

  
// 文字列描画
gdraw_string:
  long dsx#,dsy#,dsw#,dss#,dsr#
  dss#= pop dsw#= pop dsy#= pop dsx#=
  if gcolor#=COLOR_CLEAR then end
  dsx#, dsw#, + FONT_WIDTH, - dsw#=
  gdraw_string1:
    if (dss)$=NULL then end
    dsx#, dsy#, (dss)$, gdraw_font dsr#=
    dsx#, FONT_WIDTH, + dsx#=
    dss#++
  if dsx#&lt;dsw# goto gdraw_string1
  dsr#, end


// 1文字描画
gdraw_font:
  pp#= pop ty#= pop tx#=
  pp#, FONT_WIDTH, * FONT_HEIGHT, * font_area#, + qq#=
  tx#, FONT_WIDTH-1,  + rx#=
  ty#, FONT_HEIGHT-1, + ry#=
  for ii#=ty# to ry#
     for jj#=tx# to rx#
        if (qq)$&lt;&gt;0 then jj#, ii#, gdraw_point
        qq#++
     next jj#
  next ii#
  ii#, end


 .data

// 三角関数テーブル
  const TABLE_N 256 // 区分点 （全データ数＝３２１）
sin2table: // ｆ（ｘ）＝３２７６７＊ｓｉｎ（ｘ）
