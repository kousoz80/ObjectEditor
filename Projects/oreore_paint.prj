<codetext>// 元の画像の更新
gsave:
  if bitmap0#&lt;&gt;NULL then bitmap0#, free
  (bitmap)!(0), gwidth#= 
  (bitmap)!(1), gheight#=
  * 4, * 8, + tt#= malloc  bitmap0#=
  tt#, 8, / 1, - tt#=
  for ii#=0 to tt#
  (bitmap)#(ii#), (bitmap0)#(ii#)=
  next ii#
  end


// 画像のUNDO
gundo:
  if bitmap0#=NULL then end
  if bitmap#&lt;&gt;NULL then bitmap#, free
  (bitmap0)!(0), gwidth#= 
  (bitmap0)!(1), gheight#=
  * 4, * 8, + tt#= malloc  bitmap#=
  tt#, 8, / 1, - tt#=
  for ii#=0 to tt#
